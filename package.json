{
  "name": "moles",
  "version": "0.1.0",
  "description": "AI Agent that generates documentation for code repositories",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "moles": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint src/",
    "format": "biome format --write src/",
    "check": "biome check --write src/",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs"
  },
  "keywords": [
    "ai",
    "agent",
    "documentation",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@inquirer/prompts": "^8.0.2",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^17.2.3",
    "glob": "^11.0.0",
    "openai": "^6.10.0",
    "ora": "^8.1.1",
    "vitepress": "^1.5.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18"
  }
}
